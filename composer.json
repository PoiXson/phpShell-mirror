{
	"name":        "pxn/phpshell",
	"description": "Shell Utilities Library",
	"type":        "library",
	"homepage":    "https://poixson.com/",
	"license":     "AGPL-3.0",
	"keywords": [
		"framework", "utilities"
	],
	"support": {
		"source":  "https://git.poixson.com/summary/pxn!phpShell.git",
		"issues":  "https://git.poixson.com/tickets/pxn!phpShell.git",
		"discord": "https://discord.gg/jHgP2a3kDx"
	},
	"authors": [{
		"name": "lorenzop",
		"role": "Developer"
	}],
	"autoload": {
		"psr-4": {
			"pxn\\phpShell\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"pxn\\phpShell\\tests\\":    "tests/",
			"pxn\\phpShell\\examples\\": "examples/"
		}
	},
	"archive": {
		"exclude": [
			"tests/",
			"examples/"
		]
	},
	"require": {
		"pxn/phputils": "*",
		"pxn/composerlocaldev": "*"
	},
	"require-dev": {
		"nunomaduro/collision": "*",
		"phpunit/phpunit": "*"
	},
	"config": {
		"allow-plugins": {
			"pxn/composerlocaldev": true
		}
	}
}
